{
    "name": "Google Meet AutoRec",
    "description": "A Google Chrome extension to add automatic video recording for Google Meet",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "permissions": [
        "declarativeContent",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    }
}