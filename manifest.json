{
    "name": "Google Meet AutoRec",
    "description": "A Google Chrome extension to add automatic video recording for Google Meet",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "permissions": [
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "icons": {
        "128": "icon_128.png"
    }
}